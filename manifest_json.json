      },
      "universalActions": [
        {
          "label": "Settings",
          "runFunction": "onSettingsAction"
        }
      ]
    },
    "gmail": {
      "contextualTriggers": [
        {
          "unconditional": {},
          "onTriggerFunction": "onGmailContext"
        }
      ]
    }
  }
}
